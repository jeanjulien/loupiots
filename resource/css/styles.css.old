/***************************************************
				       MENU
***************************************************/

#nav {
    position:relative;
    width:auto;
    margin:0 auto;
    margin-top:15px;
    padding:10px;
}
 
ul#navigation {
    position:relative;
    float:left;
}
 
ul#navigation li {
    display:inline;
    font-size:12px;
    font-weight:bold;
    margin:0;
    padding:0;
    float:left;
    position:relative;
}

ul#navigation li#logout {
    margin-left:200;
}

 
ul#navigation li a {
    padding:10px 25px;
    color:#616161;
    text-shadow:1px 1px 0px #ffffff;
    text-decoration:none;
    display:inline-block;
    border-right:1px solid #ffffff;
    border-left:1px solid #C2C2C2;
    border-top:1px solid #ffffff;
	background: #efefef; 
	background: linear-gradient(top, #efefef 0%, #bbbbbb 100%);  
	background: -moz-linear-gradient(top, #efefef 0%, #bbbbbb 100%); 
	background: -webkit-linear-gradient(top, #efefef 0%,#bbbbbb 100%); 
	box-shadow: 0px 0px 9px rgba(0,0,0,0.15);


    -webkit-transition:color 0.2s linear, background 0.2s linear;  
    -moz-transition:color 0.2s linear, background 0.2s linear; 
    -o-transition:color 0.2s linear, background 0.2s linear;   
    transition:color 0.2s linear, background 0.2s linear;  
}
 
ul#navigation li a:hover {
    color:#282828;
}
 
ul#navigation li a.first {
    border-left: 0 none;
}
 
ul#navigation li a.last {
    border-right: 0 none;
}
 
ul#navigation li:hover > a {
    background: #dcdcdc;
}

/* Drop-Down Navigation */
ul#navigation li:hover > ul {
    visibility:visible;
    opacity:1;
}
 
ul#navigation ul, ul#navigation ul li ul {
    list-style: none;
    margin: 0;
    padding: 0;   

    visibility:hidden;
    opacity:0;
    position: absolute;
    z-index: 199;
    width:180px;
    background:#f8f8f8;
    box-shadow:1px 1px 3px #cccccc;

    -webkit-transition:opacity 0.2s linear, visibility 0.2s linear;
    -moz-transition:opacity 0.2s linear, visibility 0.2s linear;
    -o-transition:opacity 0.2s linear, visibility 0.2s linear;
    transition:opacity 0.2s linear, visibility 0.2s linear;    
}
 
ul#navigation ul {
    top: 43px;
    left: 1px;
}
 
ul#navigation ul li ul {
    top: 0;
    left: 181px; 
}
 
ul#navigation ul li {
    clear:both;
    width:100%;
    border:0 none;
    border-bottom:1px solid #c9c9c9;
}
 
ul#navigation ul li a {
    background:none;
    padding:7px 15px;
    color:#616161;
    text-shadow:1px 1px 0px #ffffff;
    text-decoration:none;
    display:inline-block;
    border:0 none;
    float:left;
    clear:both;
    width:150px;
}

/***************************************************
				   SPECIFIC
***************************************************/
#logo{
	position:relative;
   	float:left;
   	top:8px;
	left:0px;
	width:250px;
	height:160px;
}

a.button {
	padding:7px 15px;
	margin: 5px;
    color:#616161;
    font-size: 12px;
    font-weight: bold;
    text-shadow:1px 1px 0px #ffffff;
    text-decoration:none;
    display:inline-block;
    border:1px solid #ffffff;
	background: #efefef; 
	background: linear-gradient(top, #efefef 0%, #bbbbbb 100%);  
	background: -moz-linear-gradient(top, #efefef 0%, #bbbbbb 100%); 
	background: -webkit-linear-gradient(top, #efefef 0%,#bbbbbb 100%); 
	box-shadow: 0px 0px 9px rgba(0,0,0,0.15);
	-moz-box-shadow:inset 0px 0px 9px rgba(0,0,0,0.15);
	-webkit-box-shadow:inset 0px 0px 9px rgba(0,0,0,0.15);
    -moz-border-radius:6px;
	-webkit-border-radius:6px;
	border-radius:6px;
	
	-webkit-transition:color 0.2s linear, background 0.2s linear;  
    -moz-transition:color 0.2s linear, background 0.2s linear; 
    -o-transition:color 0.2s linear, background 0.2s linear;   
    transition:color 0.2s linear, background 0.2s linear;  
	
    
}
a.button:hover {
	background: #efefef; 
	background: linear-gradient(bottom, #efefef 0%, #bbbbbb 100%);  
	background: -moz-linear-gradient(bottom, #efefef 0%, #bbbbbb 100%); 
	background: -webkit-linear-gradient(bottom, #efefef 0%,#bbbbbb 100%); 
	color:#282828;
}

